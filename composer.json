{
    "name": "yiiboot/annotated",
    "type": "library",
    "description": "the yii3 annotation parser",
    "keywords": [
        "yii",
        "yii3",
        "annotation"
    ],
    "homepage": "https://github.com/yiiboot",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://github.com/yiiboot/annotated/issues?state=open",
        "source": "https://github.com/yiiboot/annotated"
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/yiiboot"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/yiiboot"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "doctrine/annotations": "^1.13",
        "spiral/tokenizer": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.14.3",
        "roave/infection-static-analysis-plugin": "^1.16",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.18"
    },
    "autoload": {
        "psr-4": {
            "Yiiboot\\Annotated\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiiboot\\Annotated\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
